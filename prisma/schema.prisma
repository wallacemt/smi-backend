generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CompanyData {
  id        String   @id @default(uuid())
  title     String
  content   String
  createdAt DateTime @default(now())
}

model CompanyAdmin {
  id              String   @id @default(uuid())
  name            String
  email           String   @unique
  password        String
  companyPosition String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
